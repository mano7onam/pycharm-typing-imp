<idea-plugin>
    <id>com.y4kstudios.pycharmtypingimp</id>
    <name>PyCharm Typing Imp</name>
    <vendor email="they4kman@gmail.com" url="https://github.com/they4kman">Y4K Studios</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.python</depends>

    <extensions defaultExtensionNs="Pythonid">
        <typeProvider implementation="com.y4kstudios.pycharmtypingimp.psi.types.GetattrTypeProvider" order="last"/>
        <typeProvider implementation="com.y4kstudios.pycharmtypingimp.psi.types.DictViewsTypeProvider" order="last"/>
    </extensions>
</idea-plugin>
